@model IEnumerable<Lodos.Models.Kategori_2>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.K2Ad)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        if (item.K1Id == 5)
        {
            var a = Convert.ToString(item.K2Id);
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.K2Ad)
                </td>
                <td>
                    @Html.ActionLink("Details", (a), "Kategori/Detail")
                </td>
            </tr>
        }
    }

</table>
